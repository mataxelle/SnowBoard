<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}SnowBoard{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body>
		<main>
			<div> {% block body %}{% endblock %}</div>
		</main>

		<footer>
			<p class="text-center">
					&copy; Copyright
					{{ 'now'|date('Y') }}
					&middot;
				<a href="/">About</a>
			</p>
		</footer>

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
    </body>
</html>
